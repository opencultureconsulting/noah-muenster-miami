                    <titleInfo lang="eng">
                        <title>Generating derived categories of groups in Kropholler&apos;s hierarchy.</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Biswas, Rudradip</displayForm>
                        <namePart type="given">Rudradip</namePart>
                        <namePart type="family">Biswas</namePart>
                        <role>
