                    <titleInfo lang="eng">
                        <title>Descent properties of topological chiral homology</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/114197584X">
