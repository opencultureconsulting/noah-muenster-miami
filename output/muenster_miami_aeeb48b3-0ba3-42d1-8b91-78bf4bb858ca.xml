                    <titleInfo lang="ger">
                        <title>Echokardiographie zur verbesserten Risikostratifizierung vor aortokoronarer Bypass- Operation</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/132043785">
