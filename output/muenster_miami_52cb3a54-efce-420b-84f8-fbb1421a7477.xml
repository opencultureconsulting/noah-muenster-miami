                    <titleInfo lang="ger">
                        <title>Robert Burns. Autor ohne Werk?</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/112168825X">
                        <displayForm>Lemke, Cordula</displayForm>
