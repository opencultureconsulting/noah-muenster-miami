                    <titleInfo lang="ger">
                        <title>Intraoperative Radiotherapie (IORT) mit Elektronen nach brusterhaltender Operation beim Mammacarcinom</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/133222233">
