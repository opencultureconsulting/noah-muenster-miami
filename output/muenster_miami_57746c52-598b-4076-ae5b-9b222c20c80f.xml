                    <titleInfo lang="ger">
                        <title>Zwischen Kunst und Kommerz – die Vettern Romberg und ihre Musikverleger</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/134827635">
                        <displayForm>Rosenberger, Burkard</displayForm>
                        <namePart type="given">Burkard</namePart>
                        <namePart type="family">Rosenberger</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">lecture</genre>
