                    <titleInfo lang="eng">
                        <title>Supporting start-ups in the process industries with accelerator programs: Types, design elements and success measurement</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1022234110">
