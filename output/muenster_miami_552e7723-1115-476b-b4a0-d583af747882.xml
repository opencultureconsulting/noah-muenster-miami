                    <titleInfo lang="eng">
                        <title>Controlling marriages: Friedrich Hagenauer and the betrothal of Indigenous Western Australian women in colonial Victoria</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1037097068">
