                    <titleInfo lang="eng">
                        <title>An analogue of Raynaud’s theorem</title>
                        <subTitle>weak formal schemes and dagger spaces</subTitle>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/110488202">
