                    <titleInfo lang="ger">
                        <title>Technologien der Autorschaft. Foucault und die russischen Formalisten</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1048415384">
