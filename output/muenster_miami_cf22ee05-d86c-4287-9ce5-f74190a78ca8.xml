                    <titleInfo lang="eng">
                        <title>An improvement of de Jong-Oort’s purity theorem</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1138154881">
