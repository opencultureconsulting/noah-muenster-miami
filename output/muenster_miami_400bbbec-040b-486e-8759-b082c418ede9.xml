                    <titleInfo lang="eng">
                        <title>Reducibility of low-dimensional Poincaré duality spaces</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1031539433">
                        <displayForm>Land, Markus</displayForm>
                        <namePart type="given">Markus</namePart>
                        <namePart type="family">Land</namePart>
                        <role>
