                    <titleInfo lang="ger">
                        <title>Entwurf eines formalen Kategoriensystems im Anschluß an Frege</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/112177123">
                        <displayForm>Hohelüchter, Martin</displayForm>
