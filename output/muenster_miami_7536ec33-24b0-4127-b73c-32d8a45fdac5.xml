                    <titleInfo lang="ger">
                        <title>Vergleich des Einflusses von Kalziumcarbonat, Putamen ovi und Estrogen auf Osteoblasten in vitro</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/12973909X">
