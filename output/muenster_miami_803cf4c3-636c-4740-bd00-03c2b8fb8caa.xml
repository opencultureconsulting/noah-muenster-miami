                    <titleInfo lang="eng">
                        <title>An identification of the Baum-Connes and Davis-Lück assembly maps</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Kranz, Julian</displayForm>
                        <namePart type="given">Julian</namePart>
                        <namePart type="family">Kranz</namePart>
