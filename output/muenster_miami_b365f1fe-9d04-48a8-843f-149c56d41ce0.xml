                    <titleInfo lang="ger">
                        <title>Neujahr in Nubien</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1053340109">
                        <displayForm>Lohwasser, Angelika</displayForm>
