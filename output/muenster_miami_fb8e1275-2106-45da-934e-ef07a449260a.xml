                    <titleInfo lang="eng">
                        <title>Nuclear dimension and n-comparison</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1138155098">
