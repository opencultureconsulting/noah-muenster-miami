                    <titleInfo lang="ger">
                        <title>Entwicklung und Charakterisierung optimierter Entladungszylinder für die ICP-Spektrometrie</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/12488847X">
