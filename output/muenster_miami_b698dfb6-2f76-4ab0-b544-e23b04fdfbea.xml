                    <titleInfo lang="eng">
                        <title>L²-Betti numbers of R-spaces and the integral foliated simplicial volume</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/130094021">
