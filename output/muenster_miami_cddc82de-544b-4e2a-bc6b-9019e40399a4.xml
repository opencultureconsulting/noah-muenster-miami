                    <titleInfo lang="eng">
                        <title>Count Data Time Series Modelling in Julia—The CountTimeSeries.jl Package and Applications</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Stapper, Manuel</displayForm>
                        <namePart type="given">Manuel</namePart>
                        <namePart type="family">Stapper</namePart>
                        <role>
