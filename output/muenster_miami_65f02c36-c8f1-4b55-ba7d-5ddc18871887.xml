                    <titleInfo lang="eng">
                        <title>Separable and tree-like asymptotic cones of groups</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1080490957">
                        <displayForm>Sisto, Alessandro</displayForm>
