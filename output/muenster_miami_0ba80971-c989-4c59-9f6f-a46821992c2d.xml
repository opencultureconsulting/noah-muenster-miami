                        <topic>Modelltheorie</topic>
                        <topic>Zil&apos;bers Trichotomie Vermutung</topic>
                        <topic>üppige Hierarchie</topic>
                        <topic>Hrushovski-Konstruktionen</topic>
                        <topic>streng minimale Theorien</topic>
                    </subject>
