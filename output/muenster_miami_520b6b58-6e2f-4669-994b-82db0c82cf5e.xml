                    <titleInfo lang="eng">
                        <title>An exploration of the ankle-biters and their role in business ecosystems</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/107010342X">
