                    <titleInfo lang="eng">
                        <title>Certain actions of finite abelian groups on higher dimensional noncommutative tori</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>He, Zhuofeng</displayForm>
                        <namePart type="given">Zhuofeng</namePart>
                        <namePart type="family">He</namePart>
                        <role>
