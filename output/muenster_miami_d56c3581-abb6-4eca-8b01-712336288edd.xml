                    <titleInfo lang="eng">
                        <title>Tangent spaces to the Teichmüller space from the energy-conscious perspective</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/123944222X">
