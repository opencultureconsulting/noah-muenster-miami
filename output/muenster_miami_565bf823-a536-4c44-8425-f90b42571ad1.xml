                    <titleInfo lang="eng">
                        <title>Equivariant homology theories for totally disconnected groups</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/12437977X">
