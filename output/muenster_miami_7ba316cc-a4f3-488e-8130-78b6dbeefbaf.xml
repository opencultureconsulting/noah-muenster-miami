                    <titleInfo lang="ger">
                        <title>Häresie – Kirchliche Normbegründung im Mittelalter zwischen Recht und Religion</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1076087728">
                        <displayForm>Steckel, Sita</displayForm>
                        <namePart type="given">Sita</namePart>
                        <namePart type="family">Steckel</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">bookPart</genre>
