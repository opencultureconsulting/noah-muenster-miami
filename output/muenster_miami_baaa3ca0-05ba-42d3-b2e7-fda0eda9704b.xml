                <mods xmlns="http://www.loc.gov/mods/v3" version="3.7" xmlns:vl="http://visuallibrary.net/vl">
                    <titleInfo lang="ger">
                        <title>Entwicklung und Validierung einer neuen Messmethode zur Bestimmung luftgetragener aliphatischer Aldehyde</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/140245456">
                        <displayForm>Dresen, Christoph</displayForm>
