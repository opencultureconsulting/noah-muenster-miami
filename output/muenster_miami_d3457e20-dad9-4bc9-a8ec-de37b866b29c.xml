                    <titleInfo lang="eng">
                        <title>L²-Betti numbers of coamenable quantum groups</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1129239217">
