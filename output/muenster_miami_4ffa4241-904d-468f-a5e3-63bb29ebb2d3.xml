                    <titleInfo lang="eng">
                        <title>Flawless start-up of production plants in process industries: The link between successful project performance and optimal future operations</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Tuin, Richard</displayForm>
                        <namePart type="given">Richard</namePart>
