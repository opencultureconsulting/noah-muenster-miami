                        <topic>positive Skalarkrümmung</topic>
                        <topic>Index Differenz</topic>
                        <topic>KK-Theorie</topic>
                        <topic>unbeschränktes Kasparov Produkt</topic>
                        <topic>stetige Felder von Hilbert Modulen</topic>
                    </subject>
