                        <topic>Modulstack globaler G-Shtukas</topic>
                        <topic>Lokale Shtukas</topic>
                        <topic>Newton Strata</topic>
                        <topic>Zentrales Blatt</topic>
                        <topic>Igusa-Varietäten</topic>
                    </subject>
