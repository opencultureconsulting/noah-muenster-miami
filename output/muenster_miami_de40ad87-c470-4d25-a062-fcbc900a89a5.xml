                    <titleInfo lang="eng">
                        <title>Mean curvature flow of cones near minimal cones</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1190115859">
