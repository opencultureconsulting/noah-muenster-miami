                    <titleInfo lang="ger">
                        <title>Ist Außenwirtschaftstheorie genderblind?</title>
                        <subTitle>Ein Beitrag zu Gender in der Makroökonomik</subTitle>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1033005045">
                        <displayForm>Fraune, Cornelia</displayForm>
                        <namePart type="given">Cornelia</namePart>
