                    <titleInfo lang="ger">
                        <title>Interaktion systemisch wirksamer Antimykotika mit der Aggregation humaner Thrombozyten in vitro</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1014041236">
