                    <titleInfo lang="ger">
                        <title>Pflichten gegen sich selbst und die Frage nach dem guten Leben</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1027956998">
                        <displayForm>Hoesch, Matthias</displayForm>
                        <namePart type="given">Matthias</namePart>
                        <namePart type="family">Hoesch</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">bookPart</genre>
                    <originInfo>
