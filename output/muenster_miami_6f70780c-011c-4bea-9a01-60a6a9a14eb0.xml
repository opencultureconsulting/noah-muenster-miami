                    <titleInfo lang="eng">
                        <title>On crystalline representations and filtered isocrystals with rational Hodge-Tate weights</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/117816988X">
