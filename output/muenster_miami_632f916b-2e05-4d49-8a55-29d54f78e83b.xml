                    <titleInfo lang="ger">
                        <title>Entwicklungstendenzen in der Lexik der deutschen Fachsprache des Flugzeugbaus</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1140833227">
