                    <titleInfo lang="eng">
                        <title>On the structure of the pro-p Iwahori-Hecke Ext-algebra</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1255008334">
