                    <titleInfo lang="ger">
                        <title>Untersuchung des Phospholipase-C-γ-2-Gens auf genetische Variabilität</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1047069954">
