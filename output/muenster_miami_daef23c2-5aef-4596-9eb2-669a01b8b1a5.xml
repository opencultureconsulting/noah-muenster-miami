                    <titleInfo lang="ger">
                        <title>Untersuchung über den Fluorid- und Boratgehalt des Trinkwassers aus privaten Wasserversorgungsanlagen</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/129459585">
