                    <titleInfo lang="eng">
                        <title>Wilhelm Bousset’s Die Religion des Judentums im neutestamentlichen Zeitalter</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/121473317">
