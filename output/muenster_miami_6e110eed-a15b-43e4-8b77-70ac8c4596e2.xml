                    <titleInfo lang="eng">
                        <title>Z-stability, finite dimensional tracial boundaries and continuous rank functions</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1138342378">
                        <displayForm>Jacelon, Bhishan</displayForm>
