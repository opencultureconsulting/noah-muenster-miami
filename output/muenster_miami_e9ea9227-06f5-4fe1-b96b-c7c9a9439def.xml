                    <titleInfo lang="eng">
                        <title>An excision theorem for the K-theory of C*-algebras, with applications to groupoid C*-algebras</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1060351560">
                        <displayForm>Putnam, Ian Fraser</displayForm>
                        <namePart type="given">Ian Fraser</namePart>
                        <namePart type="family">Putnam</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">article</genre>
                    <originInfo>
                        <dateIssued encoding="w3cdtf" keyDate="yes">2021</dateIssued>
