                    <titleInfo lang="eng">
                        <title>Food Consumption in Cologne Ehrenfeld: Gentrification through Gastrofication?</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/112717052X">
