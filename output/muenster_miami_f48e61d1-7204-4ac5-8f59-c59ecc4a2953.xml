                    <titleInfo lang="ger">
                        <title>Ideen zu einer Literatursoziologie des Internets</title>
                        <subTitle>Mit einer Blogotop-Analyse</subTitle>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/139145117">
