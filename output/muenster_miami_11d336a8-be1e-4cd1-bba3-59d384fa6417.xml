                    <titleInfo lang="ger">
                        <title>Modellierung der Bildung und Dynamik komplexer Muster in ZnS:Mn Dünnschicht-Strukturen</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/133259188">
