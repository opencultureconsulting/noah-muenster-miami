                    <titleInfo lang="ger">
                        <title>Untersuchungen zur Regulation der Apoptose in humanen Monozyten durch Statine und Immunsuppressiva</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/133991555">
