                    <titleInfo lang="eng">
                        <title>Conceptual models as basis for integrated information warehouse development</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/121064727">
