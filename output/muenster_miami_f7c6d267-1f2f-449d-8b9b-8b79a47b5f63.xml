                    <titleInfo lang="eng">
                        <title>Technology transfer by new ventures within the chemical and pharmaceutical industry</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/123291399">
