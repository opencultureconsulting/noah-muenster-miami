                    <titleInfo lang="ger">
                        <title>Braucht Nordrhein-Westfalen ein Haus der Geschichte?</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/118179918">
                        <displayForm>Ribhegge, Wilhelm</displayForm>
                        <namePart type="given">Wilhelm</namePart>
                        <namePart type="family">Ribhegge</namePart>
                        <role>
