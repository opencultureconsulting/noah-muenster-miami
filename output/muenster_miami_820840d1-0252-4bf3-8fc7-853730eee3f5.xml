                    <titleInfo lang="eng">
                        <title>An algebraic characterization of the Weyl curvature of Sm × Sm</title>
                    </titleInfo>
                    <titleInfo lang="eng" type="alternative">
                        <title>An algebraic characterization of the Weyl curvature of S m × S m</title>
                    </titleInfo>
