                    <titleInfo lang="eng">
                        <title>Elements with north-south dynamics and free subgroups have positive density in Thompson&apos;s groups T and V</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1025025350">
