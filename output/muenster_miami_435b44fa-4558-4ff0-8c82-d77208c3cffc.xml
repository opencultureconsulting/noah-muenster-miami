                        <topic>elektronische Oberflächenzustände</topic>
                        <topic>spinaufgelöste Inverse Photoemission</topic>
                        <topic>Rashba-Systeme</topic>
                        <topic>Spinpolarisations-Detektor</topic>
                        <topic>Austausch-Wechselwirkung</topic>
                        <topic>Spin-Bahn-Wechselwirkung</topic>
                    </subject>
