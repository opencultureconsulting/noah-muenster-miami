                    <titleInfo lang="eng">
                        <title>Cuntz-Li relations, inverse semigroups and groupoids</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/1026804922">
