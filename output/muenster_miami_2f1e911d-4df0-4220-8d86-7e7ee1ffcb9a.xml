                    <titleInfo lang="ger">
                        <title>Analyse und Vergleich ausgewählter mathematischer Modelle zur Bestandskalkulation in Öffentlichen Bibliotheken</title>
                    </titleInfo>
                    <name type="personal" authority="gnd" authorityURI="http://d-nb.info/gnd/" valueURI="http://d-nb.info/gnd/134827635">
                        <displayForm>Rosenberger, Burkard</displayForm>
